<p id="notice"><%= notice %></p>

<h1>Listing News Sorts</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @news_sorts.each do |news_sort| %>
      <tr>
        <td><%= link_to 'Show', news_sort %></td>
        <td><%= link_to 'Edit', edit_news_sort_path(news_sort) %></td>
        <td><%= link_to 'Destroy', news_sort, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New News sort', new_news_sort_path %>
